                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=17.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">WITH StockCalc AS (
    SELECT
        StockItemID,
        SUM(Quantity) AS TotalStock
    FROM WideWorldImporters.Warehouse.StockItemTransactions
    GROUP BY StockItemID
)

SELECT
    SI.StockItemID                                        AS ProductID_OLTP,
    CONVERT(VARCHAR(200), SI.StockItemName)               AS ProductName,
    CONVERT(VARCHAR(100), SI.Brand)                       AS Brand,
    CONVERT(VARCHAR(100), SGName.StockGroupName)          AS Category,
    SI.UnitPrice,
    CASE
        WHEN ISNULL(SI.IsChillerStock, 0) = 1 THEN 1
        ELSE 0
    END AS IsChillerStock
FROM WideWorldImporters.Warehouse.StockItems AS SI
OUTER APPLY (
    SELECT TOP (1) SG.StockGroupName
    FROM WideWorldImporters.Warehouse.StockItemStockGroups AS SSG
    JOIN WideWorldImporters.Warehouse.StockGroups AS SG
        ON SSG.StockGroupID = SG.StockGroupID
    WHERE SSG.StockItemID = SI.StockItemID
    ORDER BY SSG.StockItemStockGroupID
) AS SGName
LEFT JOIN StockCalc SC
    ON SI.StockItemID = SC.StockItemID
ORDER BY SI.StockItemID;</property>
